<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<b:Legend>
			Jugadores
		</b:Legend>
		<b:Form type="HORIZONTAL">
			<b:Fieldset>
				<b:ControlGroup ui:field="groupNameControlGroup">
					<b:ControlLabel>
						<font color="red">*</font>
						Nombre del Jugador:
					</b:ControlLabel>

					<b:Controls>
						<b:TextBox ui:field="tbPlayerName" placeholder="Nombre del Jugador"></b:TextBox>
					</b:Controls>
				</b:ControlGroup>
                <b:ControlGroup>
                    <b:ControlLabel>Posición:</b:ControlLabel>
                    <b:Controls>
                        <b:TextBox ui:field="tbPosition" placeholder="Posición"></b:TextBox>
                    </b:Controls>
                </b:ControlGroup>
                <b:ControlGroup>
                    <b:ControlLabel>Raza:</b:ControlLabel>
                    <b:Controls>
                        <b:ListBox ui:field="lbSkills"></b:ListBox>
                    </b:Controls>
                </b:ControlGroup>
			</b:Fieldset>
		</b:Form>

	</g:HTMLPanel>
</ui:UiBinder> 